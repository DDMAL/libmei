        <zeroOrMore xmlns="http://relaxng.org/ns/structure/1.0"><choice><text/><ref name="model.textphraseLike"/></choice></zeroOrMore>
      </content><attList><attDef xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" ident="delim" usage="opt">
          <desc>indicates the delimiter used to mark the portions of text that are to be
            stacked.</desc>
        </attDef><attDef xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" ident="align" usage="opt">
