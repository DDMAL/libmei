          <valList type="closed">
            <valItem ident="brace">
              <desc>curved symbol, i.e., {.</desc>
            </valItem>
            <valItem ident="bracket">
              <desc>square symbol, i.e., [.</desc>
            </valItem>
            <valItem ident="line">
              <desc>line symbol, i.e., |.</desc>
            </valItem>
            <valItem ident="none">
              <desc>grouping symbol missing.</desc>
            </valItem>
          </valList>
        </attDef>
      </attList>
