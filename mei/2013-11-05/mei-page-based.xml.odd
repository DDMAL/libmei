            <valItem ident="smpte-24">
              <desc>SMPTE 24 Film Sync.</desc>
            </valItem>
            <valItem ident="smpte-df30">
              <desc>SMPTE 30 Drop.</desc>
            </valItem>
            <valItem ident="smpte-ndf30">
              <desc>SMPTE 30 Non-Drop.</desc>
            </valItem>
            <valItem ident="smpte-df29.97">
