      <attList xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
        <attDef ident="slash" usage="opt">
          <desc>indicates the number of slashes present.</desc>
          <datatype>
            <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.SLASH"/>
          </datatype>
        </attDef>
      </attList>
    </classSpec><classSpec rend="add" ident="att.slurpresent" module="MEI.shared" type="atts">
      <desc xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Attributes for marking the presence of a slur.</desc>
      <attList xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
        <attDef ident="slur" usage="opt">
          <desc>indicates that this element participates in a slur. If visual information about the
            slur needs to be recorded, then a &lt;slur&gt; element should be employed.</desc>
          <datatype>
            <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.SLURS"/>
          </datatype>
        </attDef>
      </attList>
    </classSpec><classSpec rend="add" ident="att.space.anl" module="MEI.shared" type="atts">
      <desc xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Analytical domain attributes.</desc>
      <classes xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
        <memberOf key="att.common.anl"/>
      </classes>
    </classSpec><classSpec rend="add" ident="att.space.ges" module="MEI.shared" type="atts">
      <desc xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Gestural domain attributes.</desc>
      <classes xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
        <memberOf key="att.duration.performed"/>
      </classes>
    </classSpec><classSpec rend="add" ident="att.space.log" module="MEI.shared" type="atts">
      <desc xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Logical domain attributes.</desc>
      <classes xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
