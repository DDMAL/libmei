      </content><attList/></elementSpec><elementSpec rend="add" ident="editionStmt" module="MEI.header"><desc xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(edition statement) – Container for meta-data pertaining to a particular edition of the
        material being described.
        </desc><classes xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
        <memberOf key="att.common"/>
        <memberOf key="att.bibl"/>
        <memberOf key="att.lang"/>
      </classes><content xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
        <oneOrMore xmlns="http://relaxng.org/ns/structure/1.0"><ref name="model.editionLike"/><zeroOrMore><ref name="respStmt"/></zeroOrMore></oneOrMore>
      </content><attList/></elementSpec><elementSpec rend="add" ident="editorialDecl" module="MEI.header"><desc xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(editorial declaration) – Used to provide details of editorial principles and practices
        applied during the encoding of musical text.
        </desc><classes xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
        <memberOf key="att.common"/>
        <memberOf key="att.bibl"/>
        <memberOf key="att.datapointing"/>
        <memberOf key="att.lang"/>
        <memberOf key="model.encodingPart"/>
      </classes><content xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
        <choice xmlns="http://relaxng.org/ns/structure/1.0"><oneOrMore><ref name="model.pLike"/></oneOrMore><group><oneOrMore><ref name="model.editorialDeclPart"/></oneOrMore><zeroOrMore><ref name="model.pLike"/></zeroOrMore></group></choice>
      </content><attList/></elementSpec><elementSpec rend="add" ident="encodingDesc" module="MEI.header"><desc xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(encoding description) – Documents the relationship between an electronic file and the
        source or sources from which it was derived as well as applications used in the
        encoding/editing process.
        </desc><classes xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
        <memberOf key="att.bibl"/>
        <memberOf key="att.common"/>
        <memberOf key="model.headerPart"/>
      </classes><content xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
        <ref xmlns="http://relaxng.org/ns/structure/1.0" name="model.encodingPart_sequenceOptional"/>
      </content><attList/></elementSpec><elementSpec rend="add" ident="ensemble" module="MEI.header"><desc xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">The name of a standard instrumental or vocal grouping, such as 'orchestra' or 'marching
        band'.
        </desc><classes xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
        <memberOf key="att.common"/>
        <memberOf key="att.authorized"/>
        <memberOf key="att.bibl"/>
        <memberOf key="att.coded"/>
      </classes><content xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
        <text xmlns="http://relaxng.org/ns/structure/1.0"/>
      </content><attList/></elementSpec><elementSpec rend="add" ident="event" module="MEI.header"><desc xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Contains a description of an event, including the dates and locations of its occurrence
        and prominent participants.</desc><classes xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
        <memberOf key="att.bibl"/>
        <memberOf key="att.calendared"/>
        <memberOf key="att.common"/>
        <memberOf key="att.datable"/>
        <memberOf key="att.edit"/>
        <memberOf key="att.lang"/>
      </classes><content xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
        <optional xmlns="http://relaxng.org/ns/structure/1.0"><ref name="model.headLike"/></optional>
        <zeroOrMore xmlns="http://relaxng.org/ns/structure/1.0"><choice><ref name="model.eventPart"/><ref name="model.pLike"/><ref name="model.tableLike"/><ref name="list"/><ref name="castList"/></choice></zeroOrMore>
