        <memberOf key="att.staffDef.vis.mensural"/>
      </classes>
      <attList xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
        <attDef ident="grid.show" usage="opt">
          <desc>determines whether to display guitar chord grids.</desc>
          <datatype>
            <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.BOOLEAN"/>
          </datatype>
        </attDef>
        <attDef ident="layerscheme" usage="opt">
          <desc>indicates the number of layers and their stem directions.</desc>
          <datatype>
            <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.LAYERSCHEME"/>
          </datatype>
        </attDef>
        <attDef ident="lines" usage="opt">
          <desc>indicates the number of staff lines.</desc>
          <datatype>
            <data xmlns="http://relaxng.org/ns/structure/1.0" type="positiveInteger"/>
          </datatype>
        </attDef>
        <attDef ident="lines.color" usage="opt">
          <desc>captures the colors of the staff lines. The value is structured; that is, it should
            have the same number of space-separated RGB values as the number of lines indicated by
            the lines attribute. A line can be made invisible by assigning it the same RGB value as
            the background, usually white.</desc>
          <datatype>
            <ref xmlns="http://relaxng.org/ns/structure/1.0" name="data.COLORS"/>
          </datatype>
        </attDef>
        <attDef ident="lines.visible" usage="opt">
